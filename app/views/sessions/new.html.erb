<h2>ログインフォーム</h2>

<%= form_for(:session, url: login_path) do |f| %>
  <div class="field">
    <%= f.label :passname,"ログインID" %>
    <%= f.text_field :passname %>
  </div>
  <div class="field">
    <%= f.label :password,"パスワード" %>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>
<p>初めての方へ <%= link_to "新規登録", new_member_path %></p>